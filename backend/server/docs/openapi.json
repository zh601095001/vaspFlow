        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        }
      },
      "PostResponse": {
        "type": "object",
        "properties": {
          "acknowledged": {
            "type": "boolean"
          },
          "insertedCount": {
            "type": "number"
          },
          "insertedIds": {
            "type": "object"
          }
        }
      },
      "PutResponse": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "acknowledged": {
              "type": "boolean"
            },
            "modifiedCount": {
              "type": "number"
            },
            "upsertedId": {
              "type": "string"
            },
            "upsertedCount": {
              "type": "number"
            },
            "matchedCount": {
              "type": "number"
            }
          }
        }
      },
      "DeleteResponse": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "acknowledged": {
              "type": "boolean"
            },
            "deletedCount": {
              "type": "number"
            }
          }
        }
      }
    }
