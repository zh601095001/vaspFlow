          "200": {
            "description": "success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Items"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "db"
        ],
        "summary": "delete",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Items"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "success",
            "content": {
